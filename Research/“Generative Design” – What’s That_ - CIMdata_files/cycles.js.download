jQuery(function() { 
	jQuery('#childtabscontainer').cycle({ 
		fx:     'fade', 
		speed:   300,
		timeout: 0,
		easing: 'easeInOutSine',
		width: null,
		height: null, 
		containerResize: 0,
		fit:1,
		pager:  '#tabs-nav',
		pagerAnchorBuilder: function(idx, slide) { 
			return '#tabs-nav li:eq(' + idx + ')'; 
		}
	});
	
	jQuery('.slideshow').cycle({ 
		fx:     'fade', 
		speed:   500,
		timeout: 5000,
		easing: 'easeInOutSine',
		width: 'null',
		height: 'null',
		containerResize: 0,
		fit:0
	});

	jQuery('#ticker-container').cycle({ 
		fx:     'fade', 
		speed:   500,
		timeout: 14000,
		easing: 'easeInOutSine',
		next: '#ticker-up',
		prev: '#ticker-down',
		width: 'null',
		height: 'null',
		containerResize: 0,
		fit:0
	});
	
	jQuery("#tabs-nav").fadeIn();
	jQuery("#tabs-content").fadeIn();
	
	jQuery('.accordian').click(function() { 
		jQuery(this).next().slideToggle(500);
	});
	
	
	if(jQuery("#RSEProTickets").length > 0) { 
		console.log(jQuery("#RSEProTickets").prepend("<option value=\"1\" selected>Please choose an event</option>"));
		jQuery(".rsform-submit-button").click(function(e) { 
			e.preventDefault();
			if(jQuery("#RSEProTickets").val() == 1) { 
				alert("Please choose an event and try again!");
			} else { 
				jQuery("#userForm").submit();
			}
		});
	}
});